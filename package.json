{
  "name": "tmux-agents",
  "displayName": "tmux-agents",
  "description": "AI Agent orchestration platform for tmux â€” monitor, route, pipeline, and control 10-50 concurrent AI agents across local and remote servers.",
  "version": "2.0.0",
  "author": "super-agent.ai",
  "publisher": "super-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/super-agent-ai/tmux-agents.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/extension-icon.png",
  "activationEvents": [
    "onView:tmux-agents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tmux Agents",
      "properties": {
        "tmuxAgents.sshServers": {
          "type": "array",
          "default": [],
          "description": "SSH remote servers to monitor for tmux sessions.",
          "items": {
            "type": "object",
            "required": [
              "label",
              "host"
            ],
            "properties": {
              "label": {
                "type": "string",
                "description": "Display name for this server in the tree view (e.g., 'Production', 'Dev Box')."
              },
              "host": {
                "type": "string",
                "description": "SSH hostname or IP address."
              },
              "port": {
                "type": "number",
                "default": 22,
                "description": "SSH port number. Defaults to 22."
              },
              "user": {
                "type": "string",
                "description": "SSH username. If omitted, the system default or ~/.ssh/config will be used."
              },
              "identityFile": {
                "type": "string",
                "description": "Absolute path to the SSH private key file (e.g., '~/.ssh/id_rsa'). If omitted, default SSH keys are used."
              },
              "configFile": {
                "type": "string",
                "description": "Absolute path to a custom SSH config file (e.g., '~/.ssh/config_custom'). If omitted, the default SSH config is used."
              },
              "enabled": {
                "type": "boolean",
                "default": true,
                "description": "Whether this server is active. Set to false to temporarily disable without removing the entry."
              }
            },
            "additionalProperties": false
          }
        },
        "tmuxAgents.showLocalSessions": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show local tmux sessions. Disable if you only want to monitor remote servers."
        },
        "tmuxAgents.daemonRefresh.lightInterval": {
          "type": "number",
          "default": 10000,
          "description": "Light refresh interval in milliseconds (checks for changes)"
        },
        "tmuxAgents.daemonRefresh.fullInterval": {
          "type": "number",
          "default": 60000,
          "description": "Full refresh interval in milliseconds (complete tree rebuild)"
        },
        "tmuxAgents.daemonRefresh.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable daemon-pattern background refresh with change detection"
        },
        "tmuxAgents.paneCapture.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable pane content capture for AI status detection"
        },
        "tmuxAgents.paneCapture.lines": {
          "type": "number",
          "default": 50,
          "description": "Number of lines to capture from each pane for status detection"
        },
        "tmuxAgents.smartAttachment.openInEditor": {
          "type": "boolean",
          "default": true,
          "description": "Open tmux terminals in editor area tabs instead of bottom panel"
        },
        "tmuxAgents.orchestrator.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the agent orchestrator (brain) for automated task routing and dispatch"
        },
        "tmuxAgents.orchestrator.pollingInterval": {
          "type": "number",
          "default": 5000,
          "description": "Agent state polling interval in milliseconds"
        },
        "tmuxAgents.orchestrator.autoDispatch": {
          "type": "boolean",
          "default": true,
          "description": "Automatically dispatch pending tasks to idle agents"
        },
        "tmuxAgents.agentTemplates": {
          "type": "array",
          "default": [],
          "description": "Custom agent templates for quick spawning"
        },
        "tmuxAgents.aiProviders.claude.command": {
          "type": "string",
          "default": "claude",
          "description": "CLI command to launch Claude (e.g., 'claude', 'ddclaude', '/usr/local/bin/claude')"
        },
        "tmuxAgents.aiProviders.claude.args": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": [],
          "description": "Additional arguments for Claude. String or array of strings (e.g., [\"--model\", \"opus\", \"--verbose\"])"
        },
        "tmuxAgents.aiProviders.claude.forkArgs": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": ["--continue"],
          "description": "Arguments for forking/continuing a Claude session. String or array."
        },
        "tmuxAgents.aiProviders.claude.env": {
          "type": "object",
          "default": {},
          "description": "Environment variables set before launching Claude (e.g., {\"ANTHROPIC_API_KEY\": \"sk-...\"})",
          "additionalProperties": { "type": "string" }
        },
        "tmuxAgents.aiProviders.gemini.command": {
          "type": "string",
          "default": "gemini",
          "description": "CLI command to launch Gemini"
        },
        "tmuxAgents.aiProviders.gemini.args": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": [],
          "description": "Additional arguments for Gemini. String or array of strings."
        },
        "tmuxAgents.aiProviders.gemini.forkArgs": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": [],
          "description": "Arguments for forking/continuing a Gemini session. String or array."
        },
        "tmuxAgents.aiProviders.gemini.env": {
          "type": "object",
          "default": {},
          "description": "Environment variables set before launching Gemini",
          "additionalProperties": { "type": "string" }
        },
        "tmuxAgents.aiProviders.codex.command": {
          "type": "string",
          "default": "codex",
          "description": "CLI command to launch Codex"
        },
        "tmuxAgents.aiProviders.codex.args": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": [],
          "description": "Additional arguments for Codex. String or array of strings."
        },
        "tmuxAgents.aiProviders.codex.forkArgs": {
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ],
          "default": [],
          "description": "Arguments for forking/continuing a Codex session. String or array."
        },
        "tmuxAgents.aiProviders.codex.env": {
          "type": "object",
          "default": {},
          "description": "Environment variables set before launching Codex",
          "additionalProperties": { "type": "string" }
        }
      }
    },
