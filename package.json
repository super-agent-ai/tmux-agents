  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && mkdir -p out/core && cp node_modules/sql.js/dist/sql-wasm.wasm out/core/ && cp node_modules/sql.js/dist/sql-wasm.js out/core/ && cp node_modules/sql.js/dist/sql-wasm.wasm out/ && cp node_modules/sql.js/dist/sql-wasm.js out/ && mkdir -p out/prompts && cp src/prompts/defaults.json out/prompts/",
    "compile:mcp": "tsc -p tsconfig.mcp.json",
    "compile:all": "npm run compile && npm run compile:packages",
    "compile:packages": "npm run compile -w packages/cli && npm run compile -w packages/tui && npm run compile -w packages/mcp && npm run compile -w packages/k8s-runtime",
    "build:cli": "npm run build -w packages/cli",
    "build:tui": "npm run build -w packages/tui",
    "build:mcp": "npm run build -w packages/mcp",
    "build:k8s": "npm run build -w packages/k8s-runtime",
    "build:all": "npm run compile:all",
    "watch": "tsc -watch -p ./",
    "watch:mcp": "tsc -watch -p tsconfig.mcp.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:packages": "npm run test -w packages/cli && npm run test -w packages/tui && npm run test -w packages/mcp",
    "test:all": "npm run test && npm run test:packages",
    "test:integration": "vscode-test",
    "publish:cli": "npm publish -w packages/cli",
    "publish:tui": "npm publish -w packages/tui",
