{
  "name": "tmux-agents",
  "displayName": "tmux-agents",
  "description": "AI Agent orchestration platform for tmux — monitor, route, pipeline, and control 10-50 concurrent AI agents across local and remote servers.",
  "version": "0.1.19",
  "author": "super-agent.ai",
  "publisher": "super-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/super-agent-ai/tmux-agents.git"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/extension-icon.png",
  "activationEvents": [
    "onView:tmux-agents"
  ],
  "main": "./out/extension.js",
  "bin": {
    "tmux-agents-mcp": "./packages/mcp/dist/server.js"
  },
  "contributes": {
    "configuration": {
      "title": "Tmux Agents",
      "properties": {
        "tmuxAgents.daemonUrl": {
          "type": "string",
          "default": "",
          "description": "Remote daemon address (e.g. 192.168.1.10:3456). When set, the extension connects to a remote daemon via HTTP+WebSocket instead of the local Unix socket. Leave empty for local daemon."
        },
        "tmuxAgents.serverAliases": {
          "type": "object",
          "default": {},
          "description": "Maps daemon runtime IDs to VS Code SSH config aliases. Use when the daemon runs on a different machine and VS Code needs a different SSH host to reach the same servers. Example: { \"local\": \"mini-server\", \"dev1\": \"dev2\" } — VS Code will 'ssh mini-server' to reach the daemon host, and 'ssh dev2' to reach the server the daemon knows as dev1.",
          "additionalProperties": {
            "type": "string",
            "description": "VS Code SSH config alias for this daemon runtime"
          }
        },
        "tmuxAgents.dataDir": {
          "type": "string",
          "default": "~/.tmux-agents",
          "description": "Directory for the shared database and daemon files. Both the VS Code plugin and the daemon use this path. The database file is 'data.db' inside this directory."
        },
        "tmuxAgents.daemonRefresh": {
          "type": "object",
          "default": {
            "enabled": true,
            "lightInterval": 10000,
            "fullInterval": 60000
          },
          "description": "Background auto-refresh configuration.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable daemon-pattern background refresh with change detection"
            },
            "lightInterval": {
              "type": "number",
              "default": 10000,
              "description": "Light refresh interval in milliseconds (checks for changes)"
            },
            "fullInterval": {
              "type": "number",
              "default": 60000,
              "description": "Full refresh interval in milliseconds (complete tree rebuild)"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.paneCapture": {
          "type": "object",
          "default": {
            "enabled": true,
            "lines": 50
          },
          "description": "Pane content capture for AI status detection.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable pane content capture for AI status detection"
            },
            "lines": {
              "type": "number",
              "default": 50,
              "description": "Number of lines to capture from each pane"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.smartAttachment.openInEditor": {
          "type": "boolean",
          "default": true,
          "description": "Open tmux terminals in editor area tabs instead of bottom panel"
        },
        "tmuxAgents.orchestrator": {
          "type": "object",
          "default": {
            "enabled": true,
            "pollingInterval": 5000,
            "autoDispatch": true
          },
          "description": "Agent orchestrator configuration.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable the agent orchestrator for automated task routing and dispatch"
            },
            "pollingInterval": {
              "type": "number",
              "default": 5000,
              "description": "Agent state polling interval in milliseconds"
            },
            "autoDispatch": {
              "type": "boolean",
              "default": true,
              "description": "Automatically dispatch pending tasks to idle agents"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.agentTemplates": {
          "type": "array",
          "default": [],
          "description": "Custom agent templates for quick spawning"
        },
        "tmuxAgents.cliLaunchDelayMs": {
          "type": "number",
          "default": 3000,
          "minimum": 500,
          "maximum": 30000,
          "description": "Milliseconds to wait after creating a tmux window before sending the AI CLI command and prompt."
        }
      }
    },
