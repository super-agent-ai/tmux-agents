{
  "name": "@tmux-agents/tui",
  "version": "0.1.19",
  "description": "Terminal UI for tmux-agents daemon",
  "type": "module",
  "main": "tui.cjs",
  "bin": {
    "tmux-agents-tui": "./tui.cjs"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && mkdir -p dist/core && cp ../../node_modules/sql.js/dist/sql-wasm.wasm dist/core/ && cp ../../node_modules/sql.js/dist/sql-wasm.js dist/core/",
    "compile": "npm run build",
    "watch": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tmux",
    "agents",
    "ai",
    "tui",
    "terminal"
  ],
  "author": "super-agent.ai",
  "license": "MIT",
  "dependencies": {
    "ink": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.0.0",
    "sql.js": "^1.14.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "happy-dom": "^15.0.0",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.8.0",
    "vitest": "^2.0.0"
  }
}
