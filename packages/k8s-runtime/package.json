{
  "name": "@tmux-agents/k8s-runtime",
  "displayName": "tmux-agents",
  "description": "AI Agent orchestration platform for tmux â€” monitor, route, pipeline, and control 10-50 concurrent AI agents across local and remote servers.",
  "version": "0.1.19",
  "author": "super-agent.ai",
  "publisher": "super-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/super-agent-ai/tmux-agents.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/extension-icon.png",
  "activationEvents": [
    "onView:tmux-agents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tmux Agents",
      "properties": {
        "tmuxAgents.sshServers": {
          "type": "object",
          "default": {
            "servers": [],
            "script": {}
          },
          "description": "SSH remote servers. Static list and/or dynamic script-based discovery.",
          "properties": {
            "servers": {
              "type": "array",
              "default": [],
              "description": "Static SSH server list.",
              "items": {
                "type": "object",
                "required": [
                  "label",
                  "host"
                ],
                "properties": {
                  "label": {
                    "type": "string",
                    "description": "Display name in the tree view"
                  },
                  "host": {
                    "type": "string",
                    "description": "SSH hostname or IP address"
                  },
                  "port": {
                    "type": "number",
                    "default": 22,
                    "description": "SSH port. Default: 22"
                  },
                  "user": {
                    "type": "string",
                    "description": "SSH username"
                  },
                  "configFile": {
                    "type": "string",
                    "description": "Path to SSH config file (-F flag)"
                  },
                  "enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether this server is active"
                  }
                },
                "additionalProperties": false
              }
            },
            "script": {
              "type": "object",
              "default": {},
              "description": "Script-based SSH server discovery. Script must output JSON array of { label, host, ... } to stdout.",
              "properties": {
                "path": {
                  "type": "string",
                  "description": "Path to the script (e.g., '~/.config/tmux-agents/servers.sh')"
                },
                "interval": {
                  "type": "number",
                  "default": 300,
                  "minimum": 10,
                  "description": "Re-run interval in seconds. Default: 300"
                },
                "timeout": {
                  "type": "number",
                  "default": 10,
                  "minimum": 1,
                  "maximum": 60,
                  "description": "Max wait for script in seconds. Default: 10"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.showLocalSessions": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show local tmux sessions. Disable if you only want to monitor remote servers."
        },
        "tmuxAgents.daemonRefresh": {
          "type": "object",
          "default": {
            "enabled": true,
            "lightInterval": 10000,
            "fullInterval": 60000
          },
          "description": "Background auto-refresh configuration.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable daemon-pattern background refresh with change detection"
            },
            "lightInterval": {
              "type": "number",
              "default": 10000,
              "description": "Light refresh interval in milliseconds (checks for changes)"
            },
            "fullInterval": {
              "type": "number",
              "default": 60000,
              "description": "Full refresh interval in milliseconds (complete tree rebuild)"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.paneCapture": {
          "type": "object",
          "default": {
            "enabled": true,
            "lines": 50
          },
          "description": "Pane content capture for AI status detection.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable pane content capture for AI status detection"
            },
            "lines": {
              "type": "number",
              "default": 50,
              "description": "Number of lines to capture from each pane"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.smartAttachment.openInEditor": {
          "type": "boolean",
          "default": true,
          "description": "Open tmux terminals in editor area tabs instead of bottom panel"
        },
        "tmuxAgents.orchestrator": {
          "type": "object",
          "default": {
            "enabled": true,
            "pollingInterval": 5000,
            "autoDispatch": true
          },
          "description": "Agent orchestrator configuration.",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true,
              "description": "Enable the agent orchestrator for automated task routing and dispatch"
            },
            "pollingInterval": {
              "type": "number",
              "default": 5000,
              "description": "Agent state polling interval in milliseconds"
            },
            "autoDispatch": {
              "type": "boolean",
              "default": true,
              "description": "Automatically dispatch pending tasks to idle agents"
            }
          },
          "additionalProperties": false
        },
        "tmuxAgents.agentTemplates": {
          "type": "array",
          "default": [],
          "description": "Custom agent templates for quick spawning"
        },
        "tmuxAgents.defaultProvider": {
          "type": "string",
          "default": "claude",
          "enum": [
            "claude",
            "gemini",
            "codex",
            "opencode",
            "cursor",
            "copilot",
            "aider",
            "amp",
            "cline",
            "kiro"
          ],
          "description": "Default AI provider used for tasks and swimlanes when no provider is explicitly set."
        },
        "tmuxAgents.fallbackProvider": {
          "type": "string",
          "default": "gemini",
          "enum": [
            "claude",
            "gemini",
            "codex",
            "opencode",
            "cursor",
            "copilot",
            "aider",
            "amp",
            "cline",
            "kiro"
          ],
          "description": "Fallback AI provider used when the default provider fails or is unavailable."
        },
        "tmuxAgents.defaultPromptsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable built-in default prompt templates (Create Test Plans, Auto-Pass Tests, Install Plugins). Disable for gradual rollout."
        },
        "tmuxAgents.cliLaunchDelayMs": {
          "type": "number",
          "default": 3000,
          "minimum": 500,
          "maximum": 30000,
          "description": "Milliseconds to wait after creating a tmux window before sending the AI CLI command and prompt."
        },
        "tmuxAgents.aiProviders": {
          "type": "object",
          "default": {
            "claude": {
              "command": "claude",
              "pipeCommand": "claude",
              "args": [],
              "forkArgs": [
                "--continue"
              ],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--dangerously-skip-permissions"
              ],
              "env": {}
            },
            "gemini": {
              "command": "gemini",
              "pipeCommand": "gemini",
              "args": [],
              "forkArgs": [],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--yolo"
              ],
              "env": {}
            },
            "codex": {
              "command": "codex",
              "pipeCommand": "codex",
              "args": [],
              "forkArgs": [],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--full-auto"
              ],
              "env": {}
            },
            "opencode": {
              "command": "opencode",
              "pipeCommand": "opencode",
              "args": [],
              "forkArgs": [
                "--continue"
              ],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--yolo"
              ],
              "env": {}
            },
            "cursor": {
              "command": "agent",
              "pipeCommand": "agent",
              "args": [],
              "forkArgs": [
                "--resume"
              ],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--yolo"
              ],
              "env": {}
            },
            "copilot": {
              "command": "copilot",
              "pipeCommand": "copilot",
              "args": [],
              "forkArgs": [
                "--continue"
              ],
              "resumeFlag": "--resume",
              "autoPilotFlags": [],
              "env": {}
            },
            "aider": {
              "command": "aider",
              "pipeCommand": "aider",
              "args": [],
              "forkArgs": [],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--yes-always"
              ],
              "env": {}
            },
            "amp": {
              "command": "amp",
              "pipeCommand": "amp",
              "args": [],
              "forkArgs": [],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--dangerously-allow-all"
              ],
              "env": {}
            },
            "cline": {
              "command": "cline",
              "pipeCommand": "cline",
              "args": [],
              "forkArgs": [],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--yolo"
              ],
              "env": {}
            },
            "kiro": {
              "command": "kiro-cli",
              "pipeCommand": "kiro-cli",
              "args": [],
              "forkArgs": [
                "--resume"
              ],
              "resumeFlag": "--resume",
              "autoPilotFlags": [
                "--trust-all-tools"
              ],
              "env": {}
            }
          },
          "description": "AI provider configurations. Each key (claude, gemini, codex, opencode, cursor, copilot, aider, amp, cline, kiro) supports: command (interactive CLI), pipeCommand (pipe mode CLI), args (launch arguments), forkArgs (fork/continue arguments), env (environment variables).",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "command": {
                "type": "string",
                "description": "CLI command for interactive tmux sessions"
              },
              "pipeCommand": {
                "type": "string",
                "description": "CLI command for pipe mode (AI Generate, summarize)"
              },
              "args": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                ],
                "description": "Additional arguments for launch. String or array of strings."
              },
              "forkArgs": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                ],
                "description": "Arguments for forking/continuing a session (no session ID). String or array."
              },
              "resumeFlag": {
                "type": "string",
                "description": "Flag to resume a specific session by ID (e.g. '--resume'). Used as: command <resumeFlag> <sessionId>."
              },
              "autoPilotFlags": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                ],
                "description": "CLI flags appended when Pilot toggle is enabled (e.g. '--dangerously-skip-permissions' for Claude). String or array of strings."
              },
              "env": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                },
                "description": "Environment variables set before launching"
              },
              "defaultWorkingDirectory": {
                "type": "string",
                "description": "Default working directory for pipe mode (AI Chat, AI Generate). Falls back to the first open workspace folder."
              },
              "shell": {
                "type": "boolean",
                "default": false,
                "description": "Run pipe commands through the system shell. Enable if pipeCommand is a shell script or needs shell features."
              }
            },
            "additionalProperties": false
          }
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tmux-agents",
          "title": "Tmux Agents",
          "icon": "resources/icon.svg"
