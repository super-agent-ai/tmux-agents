{
  "name": "@tmux-agents/cli",
  "version": "0.1.19",
  "description": "Command-line interface for tmux-agents daemon",
  "main": "dist/cli/index.js",
  "bin": {
    "tmux-agents": "./dist/cli/cli/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && mkdir -p dist/cli/core && cp ../../node_modules/sql.js/dist/sql-wasm.wasm dist/cli/core/ && cp ../../node_modules/sql.js/dist/sql-wasm.js dist/cli/core/ && chmod +x dist/cli/cli/index.js",
    "compile": "npm run build",
    "watch": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["tmux", "agents", "ai", "cli"],
  "author": "super-agent.ai",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "sql.js": "^1.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.8.0",
    "vitest": "^2.0.0"
  }
}
